function result = matchAndExtract(A, B)
    % Initialize a result array with NaN values
    result = NaN(size(B));

    % Find the indices where B matches A's first column
    [~, idx] = ismember(B, A(:, 1));

    % Replace NaN values in the result array with corresponding values from A's second column
    result(~isnan(idx)) = A(idx(~isnan(idx)), 2);
end
